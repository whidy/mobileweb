  "license": "ISC",
  "bugs": {
    "url": "https://github.com/whidy/mobileweb/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "homepage": "https://github.com/whidy/mobileweb#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "clean-webpack-plugin": "*",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.5",
    "expose-loader": "^3.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jquery": "^3.6.0",
